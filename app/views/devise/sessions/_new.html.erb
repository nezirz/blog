<h4>Log In</h4>
<hr>
<%= form_for(:user, :url => session_path(:user)) do |f| %>
  <%= f.text_field :email %><br/>
  <%= f.password_field :password %><br/>
  <%= f.check_box :remember_me %>
  <%= f.label :remember_me , {:class=>"font8"}  %>
  <%= link_to "Forgot your password?", new_password_path(:user), {:class=>"font8"}   %><br/>
  <%= f.submit 'Sign in' %>
<% end %>
